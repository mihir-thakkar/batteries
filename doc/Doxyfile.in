PROJECT_NAME = "BATTERIES"

OUTPUT_DIRECTORY = @CMAKE_CURRENT_BINARY_DIR@/doc_doxygen/
INPUT = @CMAKE_CURRENT_SOURCE_DIR@/../src/ @CMAKE_CURRENT_SOURCE_DIR@/../doc/doxygen
INPUT_ENCODING = UTF-8
FILE_PATTERNS = *.c *.cpp *.h *.hpp *.ipp *.y *.html *.css *.js *.xml *.md
RECURSIVE = YES
EXCLUDE =
EXCLUDE_SYMLINKS = NO
EXCLUDE_PATTERNS = *_test.*
FILTER_PATTERNS =
FILTER_SOURCE_FILES = NO
FILTER_SOURCE_PATTERNS =

STRIP_FROM_INC_PATH = @CMAKE_CURRENT_SOURCE_DIR@/

FULL_PATH_NAMES = NO
SHORT_NAMES = NO
JAVADOC_AUTOBRIEF = YES
ENABLE_PREPROCESSING = YES
MACRO_EXPANSION = YES
PREDEFINED += BATT_DOXYGEN:=1
EXAMPLE_PATH=

GENERATE_HTML = YES
GENERATE_XML = YES
GENERATE_JSON = YES
GENERATE_LATEX = NO
SOURCE_BROWSER = YES
SHOW_INCLUDE_FILES = YES
XML_NS_MEMB_FILE_SCOPE = YES